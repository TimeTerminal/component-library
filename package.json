{
  "name": "component-library",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production --optimize-minimize --optimize-occurrence-order",
    "build-prod": "NODE_ENV=production yarn run build",
    "build-stage": "NODE_ENV=staging yarn run build",
    "ci": "yarn install --frozen-lockfile",
    "start": "NODE_ENV=development webpack-dev-server --port 3001 --watch",
    "dev": "NODE_ENV=development BABEL_ENV=hot webpack-dev-server --host 0.0.0.0 --mode=development --hot --inline",
    "test": "NODE_ENV=test jest --config ./test/config.json",
    "coverage": "NODE_ENV=test jest --coverage --config ./test/config.json",
    "test-watch": "yarn test --watch",
    "test-update": "yarn test --updateSnapshot",
    "test:integration": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint ./ --ext .js --ext .jsx",
    "dev:build": "SSH_PRIVATE_KEY=\"${SSH_PRIVATE_KEY:-$(cat ~/.ssh/id_rsa)}\" docker-compose build",
    "dev:up": "docker-compose up -d",
    "dev:logs": "docker-compose logs -f web-admin",
    "dev:shell": "docker-compose exec web-admin sh",
    "dev:down": "docker-compose down",
    "dev:reset": "yarn dev:down -v;yarn dev:build && yarn dev:up"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.5.1",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "bundle-loader": "^0.5.6",
    "config": "^3.0.0",
    "crypto-js": "4.2.0",
    "file-saver": "^2.0.0",
    "flexboxgrid": "6.3.1",
    "history": "4.7.2",
    "immer": "~1.8.0",
    "lodash": "4.17.19",
    "moment": "2.29.4",
    "prop-types": "~15.6.2",
    "react": "~16.6.3",
    "react-bootstrap-table": "4.3.1",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "^16.6.3",
    "react-fontawesome": "1.6.1",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-select": "~2.1.2",
    "react-styled-flexboxgrid": "2.6.0",
    "react-tabs": "2.3.0",
    "react-toastify": "~4.4.3",
    "redux": "4.0.1",
    "redux-act": "^1.7.4",
    "redux-form": "7.4.2",
    "redux-saga": "0.16.2",
    "reselect": "4.0.0",
    "styled-components": "~4.1.2",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "8.0.4",
    "babel-plugin-styled-components": "^1.9.2",
    "clean-webpack-plugin": "1.0.0",
    "colors": "^1.3.2",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "1.0.1",
    "cypress": "~3.1.0",
    "cypress-react-unit-test": "~1.2.1",
    "enzyme": "~3.7.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "4.0.0",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^8.1.0",
    "nock": "^9.6.1",
    "node-sass": "4.14.1",
    "prettier": "^1.15.3",
    "react-hot-loader": "4.3.12",
    "react-testing-library": "^5.3.1",
    "require-hacker": "3.0.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "5.94.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.10.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn coverage"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
